{
  "type": "origins:multiple",
  "active": {
    "type": "toomanyorigins:hover",
    "min": 0,
    "max": 60,
    "start_value": 60,
    "recharge_while_inactive": true,
    "hud_render": {
      "should_render": true,
      "sprite_location": "toomanyorigins:textures/gui/tmo_resource_bar.png",
      "bar_index": 8,
      "condition": {
        "type": "origins:power_active",
        "power": "*:*_toggle"
      }
    },
    "key": {
      "key": "key.jump",
      "continuous": true
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "inactive": {
    "type": "origins:resource",
    "min": 0,
    "max": 60,
    "hud_render": {
      "should_render": true,
      "sprite_location": "toomanyorigins:textures/gui/tmo_resource_bar.png",
      "bar_index": 9,
      "condition": {
        "type": "origins:power_active",
        "power": "*:*_toggle",
        "inverted": true
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle",
      "inverted": true
    }
  },
  "sync_values": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "toomanyorigins:resource_transfer",
      "resource": "*:*_inactive",
      "provider": "*:*_active"
    },
    "interval": 1,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "toomanyorigins:compare_resource",
          "resource": "*:*_inactive",
          "compare_to": "*:*_active",
          "comparison": "==",
          "inverted": true
        },
        {
          "type": "origins:power_active",
          "power": "*:*_toggle",
          "inverted": true
        }
      ]
    }
  },
  "toggle_key": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:power_active",
        "power": "*:*_toggle"
      },
      "if_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "toomanyorigins:toggle",
            "power": "*:*_toggle"
          },
          {
            "type": "toomanyorigins:resource_transfer",
            "resource": "*:*_inactive",
            "provider": "*:*_active"
          }
        ]
      },
      "else_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "toomanyorigins:toggle",
            "power": "*:*_toggle"
          },
          {
            "type": "toomanyorigins:resource_transfer",
            "resource": "*:*_active",
            "provider": "*:*_inactive"
          }
        ]
      }
    },
    "cooldown": 1,
    "hud_render": {
      "should_render": false
    },
    "key": {
      "key": "key.origins.primary_active"
    }
  },
  "hidden": true
}