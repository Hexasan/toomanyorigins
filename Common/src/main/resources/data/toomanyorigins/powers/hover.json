{
  "type": "origins:multiple",
  "hover": {
    "type": "toomanyorigins:hover",
    "step_assist": 0.6,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "toomanyorigins:velocity",
          "y": 0.0,
          "comparison": ">="
        },
        {
          "type": "toomanyorigins:key_pressed",
          "key": {
            "key": "key.jump",
            "continuous": true
          }
        },
        {
          "type": "origins:climbing",
          "inverted": true
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": "0",
          "inverted": true
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:lava",
          "comparison": ">",
          "compare_to": "0",
          "inverted": true
        },
        {
          "type": "origins:fall_flying",
          "inverted": true
        },
        {
          "type": "origins:riding",
          "inverted": true
        },
        {
          "type": "origins:power_active",
          "power": "*:*_toggle_resource"
        }
      ]
    }
  },
  "deplete_hunger_while_using": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:or",
            "conditions": [
              {
                "type": "origins:gamemode",
                "gamemode": "creative"
              },
              {
                "type": "origins:food_level",
                "comparison": ">",
                "compare_to": 6
              }
            ],
            "inverted": true
          },
          "if_action": {
            "type": "origins:if_else",
            "condition": {
              "type": "toomanyorigins:moving"
            },
            "if_action": {
              "type": "origins:exhaust",
              "amount": 0.08
            },
            "else_action": {
              "type": "origins:exhaust",
              "amount": 0.04
            }
          }
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:using_item"
          },
          "if_action": {
            "type": "origins:exhaust",
            "amount": 0.04
          }
        }
      ]
    },
    "interval": 1,
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_hover"
    }
  },
  "toggle_resource": {
    "type": "origins:toggle",
    "active_by_default": true,
    "key": {
      "key": "key.origins.primary_active"
    }
  },
  "badges": [
    {
      "type": "toomanyorigins:defined_keybind",
      "sprite": "origins:textures/gui/badge/active.png",
      "text": "origins.gui.badge.active",
      "key": {
        "key": "key.jump"
      }
    },
    {
      "sprite": "origins:textures/gui/badge/toggle.png",
      "text": "origins.gui.badge.toggle"
    }
  ]
}